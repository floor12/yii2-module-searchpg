{
  "name": "floor12/yii2-module-searchpg",
  "description": "This module works with Postgres and help make a full text search in web site made with Yii2 framework.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2",
    "postgres",
    "postgresql",
    "full text search",
    "search"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@floor12.net",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "yii2mod/yii2-enum": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\searchpg\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\searchpg\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
